{
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/api/wish",
      "destination": "/api/wish.ts"
    },
    {
      "source": "/api/vote", 
      "destination": "/api/vote.ts"
    },
    {
      "source": "/api/og",
      "destination": "/api/og.ts"
    }
  ],
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}